#:kivy 1.1.1

<varylayout>:
    name:'varylayout'
    BoxLayout:
        id: main_layout
        orientation: 'vertical'


        TabbedPanel:
            do_default_tab: False
            background_color: 0.3, 0.6, 1, 0.5 # to change the background of the whole thing

            TabbedPanelItem:
                text: 'Diversify'
                color: (0.2, 0.2, 10, 10)
                background_color: 0.2, 0.3, 0.6, 0.6
                BoxLayout:
                    orientation: 'vertical'
                    background_color: 0.2, 0.3, 0.6, 10
