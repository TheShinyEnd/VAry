                                cols:1
                                Label:
                                    font_name: 'Quicksand'
                                    underline: True
                                    text:"Diversify WIP\nstable release?, enjoy\nDiversify Scanner, scroll to bottom"
                                    id:diversifytext
                                    bold: True

                                    color: (0.7, 0.3, 0.5, 0.7)


