#:kivy 1.1.1

<varylayout>:
    name:'varylayout'
    background_color: 0.4, 0.6, 0.8, 1
    color: 0, 0, 0, 1
    BoxLayout:
        id: main_layout
        orientation: 'vertical'
        background_color: 0.4, 0.6, 0.8, 1
        color: 0, 0, 0, 1
        TabbedPanel:
            tab_width: self.size[0]/len(self.tab_list)
            tab_height: 130
            do_default_tab: False
            background_color: 0.4, 0.6, 0.8, 1

            TabbedPanelItem:

                text: 'Diversify'
                font_name: 'Quicksand'
                color: (0.5, 0.6, 1, 1.0)   
                background_color: 0.4, 0.6, 0.8, 1  

                BoxLayout:
                    orientation: 'vertical'
                    background_color: 0.1, 0.3, 0.3, 10
