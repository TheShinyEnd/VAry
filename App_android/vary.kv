                                cols:1
                                Label:
                                    underline: True
                                    text:"Diversify WIP\nstable release?, enjoy\nDiversify Scanner, scroll to bottom"
                                    id:diversifytext
                                    bold: True

                                    color: (0.7, 0.1, 0.1, 0.7)


